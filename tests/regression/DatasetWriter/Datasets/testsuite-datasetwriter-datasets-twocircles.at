### test on classification problem: two circles

AT_SETUP([DatasetWriter - Datasets - two circles])
AT_KEYWORDS([twocircles tatidatasetwriter])

AT_CHECK([../../../TATiDatasetWriter \
	--data_type 0 \
	--dimension 20 \
	--noise 0.1 \
	--seed 426 \
	--train_test_ratio 0 \
	--test_data_files testset.csv
], 0, [stdout], [stderr])
AT_CHECK([../../../NumericalDiff -- testset.csv ${abs_top_srcdir}/tests/regression/DatasetWriter/Datasets/post/testset-twocircles.csv], 0, [ignore], [ignore])

AT_CLEANUP

