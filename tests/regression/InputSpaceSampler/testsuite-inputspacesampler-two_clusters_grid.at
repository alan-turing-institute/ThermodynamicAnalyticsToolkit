### test on

AT_SETUP([InputSpaceSampler - two clusters grid])
AT_KEYWORDS([inputspacesampler two_clusters])

AT_CHECK([../../../InputSpaceSampler \
    --batch_data_files grid.csv \
    --input_dimension 2 \
    --interval_input -4 4 \
	--samples_input 10 \
	--csv_file InputSpaceSampler-output.csv \
	--seed 426 \
	--steps 1 \
	--trajectory_file ${abs_top_srcdir}/tests/regression/InputSpaceSampler/pre/trajectory-SGLD.csv
], 0, [stdout], [stderr])

AT_CHECK([../../../NumericalDiff -- grid.csv ${abs_top_srcdir}/tests/regression/InputSpaceSampler/post/grid.csv], 0, [ignore], [ignore])
AT_CHECK([../../../NumericalDiff -- InputSpaceSampler-output.csv ${abs_top_srcdir}/tests/regression/InputSpaceSampler/post/InputSpaceSampler-output.csv], 0, [ignore], [ignore])

AT_CLEANUP

