### test on cmdline examples in the userguide

AT_SETUP([Examples cmdline - analysis averages])
AT_KEYWORDS([example cmdline analyse_average])

# analyse a trajectory that was created by:
# (Two clusters dataset with two inputs nodes, single output, linear activation)
# DDSampler \
# --run_file run-two_clusters-dim_500-gamma_10-beta_1e4-max_steps_1000-noise_0.1-linear-seed_426.csv
# --data_type 2 \
# --dimension 500 \
# --friction_constant 10 \
# --inverse_temperatur 1e4 \
# --max_steps 1000 \
# --noise 0.1 \
# --output_activation linear \
# --seed 426 \
# --trajectory_file trajectory-two_clusters-dim_500-gamma_10-beta_1e4-max_steps_1000-noise_0.1-linear-seed_426.csv
#
AT_CHECK([cp -f ${abs_top_srcdir}/tests/userguide/cmdline/pre/run-two_clusters-dim_500-gamma_10-beta_1e4-max_steps_1000-noise_0.1-linear-seed_426.csv run.csv], 0, [ignore], [ignore])
AT_CHECK([cp -f ${abs_top_srcdir}/tests/userguide/cmdline/pre/trajectory-two_clusters-dim_500-gamma_10-beta_1e4-max_steps_1000-noise_0.1-linear-seed_426.csv trajectory.csv], 0, [ignore], [ignore])

AT_CHECK([env - PATH=${AUTOTEST_PATH} /bin/bash \
	${abs_top_srcdir}/doc/userguide/cmdline/analyse_average.sh
], 0, [stdout], [stderr])

AT_CLEANUP

